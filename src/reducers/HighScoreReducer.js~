import ActionTypes from '../actions/ActionTypes';

const initialState = {
    name: '',
    highScore: 0,
    date: ''
};

export default function HighScoreReducer(state = initialState, action) {
    switch (action.type) {
    case ActionTypes.HIGH_SCORE_RETRIEVED:
	return {
	    ...state,
	    action.highScore
	};

    default:
	return state;
    };
};
